  <property name="styleSheet">
   <string notr="true">/* general styles */

* {
	color: #ffffff;
}

#sidebar {
	background-color: #1e1e1e;
}

QPushButton:focus {
    border: none;
    outline: none;
}

#content *, #login_content, #register_content, #player_page {
	background-color: #111111;
}

#sidebar QPushButton {
	font-size: 16px;
	padding: 14px;
	text-align: left;
	border-radius: 5px;
	color: #dddddd;
}

#sidebar QPushButton:hover {
	color: #ffffff;
}

#sidebar QPushButton:checked {
	background-color: #303030;
	color: #ffffff;
}

QToolButton {
	border: 0;
}

QLineEdit {
	padding: 10px
}

/* login page &amp; register page styles */
#login_content #login_page_label, #register_content #register_page_label {
	font-size: 20px;
}

#login_content *, #register_content * {
	font-size: 16px;
}

#login_content #login_form_container, #register_content #register_form_container {
	background-color: #303030;
	border-radius: 5px;
}

#login_content QLineEdit, #register_content QLineEdit {
	color: black;
	background-color: white;
	border-radius: 5px;
}

#login_content #login_button, #register_content #register_button {
	border-radius: 5px;
	background-color: rgb(0, 175, 0);
	padding: 5px; 
	margin: 0 8px 0 8px;
}

#login_content #login_button:hover, #register_content #register_button:hover {
	background-color:rgb(0, 158, 0);
}

#login_content #sign_up_container #sign_up_button,
#register_content #sign_in_container #sign_in_button {
	background-color: #303030;
	border: none;
	text-align: left;
}

#login_content #sign_up_container #sign_up_button:hover,
#register_content #sign_in_container #sign_in_button:hover {
	text-decoration: underline;
}

/* sidebar styles */

#player_thumbnail {
	background-color: #111111;
}


/* home page styles */

#home_page #home_page_scrollarea_contents &gt; QWidget {
	border-radius: 5px;
	border: 1px solid #D0D0D0;
}	

/* browse page styles */
#search_bar, #recent_searches_widget {
	border-radius: 5px;
	border: 1px solid #D0D0D0;
}

/* profile page styles */
#user_profile_button {
	font-size: 18px;
	padding: 4px;
	text-align: left;
	color: #dddddd;
	border: 0;
}

/* playlist page styles */
#playlist_contents_scrollarea {
	border: 1px solid #D0D0D0;
}

/* settings page styles */
#settings_language_page QComboBox,  
#settings_theme_page QComboBox {
	background-color: white;
	padding: 2px;
	color: black;
	border: none;
}

#settings_language_page QPushButton, 
#settings_theme_page QPushButton {
	border-radius: 5px;
	background-color: #1e1e1e;
	padding: 5px 20px;
	color: #dddddd;
}

#settings_language_page QPushButton:hover,
#settings_theme_page QPushButton:hover {
	background-color: #303030;
	color: #ffffff;
}

#settings_theme_page #settings_theme_content #theme_scrollArea #settings_theme_widget #theme_preview_content {
	border-radius: 5px;
	border: 1px solid #D0D0D0;
}	

#settings_about_page #about_page_content #about_page_scrollArea #about_content #about_us_label {
	margin: 10px 0;
	padding: 5px;
	border: 1px solid #D0D0D0;
	border-radius: 5px;
}

#settings_about_page #about_page_content #about_page_scrollArea #about_content #team_member_label
{
	margin: 10px 0;
	padding: 5px;
	border: 1px solid #D0D0D0;
	border-radius: 5px;
}</string>
